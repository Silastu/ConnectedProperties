{
  "version": "4.0.0-eta-01",
  "authors": [ "Stephen Cleary" ],
  "description": "Dynamically attached object properties.",
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "packOptions": {
    "tags": [ "dynamic", "attached", "connected", "properties" ],
    "projectUrl": "https://github.com/StephenCleary/ConnectedProperties",
    "licenseUrl": "https://raw.githubusercontent.com/StephenCleary/ConnectedProperties/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "https://github.com/StephenCleary/ConnectedProperties.git"
    }
  },
  "frameworks": {
    "netstandard1.1": {
      "dependencies": {
        "System.Collections.Concurrent": "4.0.12",
        "System.Dynamic.Runtime": "4.0.11",
        "System.Reflection.Extensions": "4.0.1",
        "System.Runtime": "4.1.0"
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile111": {
      "frameworkAssemblies": {
        "System.Collections.Concurrent": { "type": "build" },
        "System.Dynamic.Runtime": { "type": "build" },
        "System.Reflection": { "type": "build" },
        "System.Reflection.Extensions": { "type": "build" },
        "System.Runtime": { "type": "build" }
      }
    }
  }
}
